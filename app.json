{
  "expo": {
    "name": "두둑",
    "slug": "doodook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo-d-app-icon-1500.png",
    "projectId": "d831fa11-69a9-40eb-a916-ae0d22291e92",
    "splash": {
      "image": "./src/assets/images/logo-d-app-icon-1500.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.lipeuoo.doodook",
      "buildNumber": "7",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo-d-app-icon-1500.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.r8ol7z.doodook",
      "googleServicesFile": "./google-services.json",
      "permissions": ["WAKE_LOCK", "RECEIVE_BOOT_COMPLETED"]
    },
    "web": {
      "favicon": "./src/assets/images/logo-d-app-icon-1500.png"
    },
    "plugins": [
      "expo-asset",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/icons/star-filled.png",
          "color": "#F074BA"
        }
      ]
    ]
  }
}
