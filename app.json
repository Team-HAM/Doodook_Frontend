{
  "expo": {
    "name": "Doodook",
    "slug": "rncourse",
    "owner": "yehyeon",
    "extra": { "eas": { "projectId": "03400e3a-d687-464c-9ef2-13322cf99fcb" } },

    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo-d-app-icon-1500.png",
    "splash": {
      "image": "./src/assets/images/logo-d-app-icon-1500.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": { "fallbackToCacheTimeout": 0 },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.lipeuoo.doodook",
      "buildNumber": "10",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo-d-app-icon-1500.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.r8ol7z.doodook"
    },
    "web": { "favicon": "./src/assets/images/logo-d-app-icon-1500.png" },
    "plugins": [
      "expo-asset",
      ["expo-notifications", { "icon": "./src/assets/icons/star-filled.png", "color": "#F074BA" }]
    ]
  }
}
